    "@babel/parser": "^7.25.6",
    "@babel/plugin-transform-typescript": "^7.25.2",
    "@clack/prompts": "^0.10.0",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "execa": "^9.4.0",
    "recast": "^0.23.9",
    "ts-morph": "^20.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.14.12",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
